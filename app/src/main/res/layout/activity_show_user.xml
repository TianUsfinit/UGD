<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ShowUser">

    <Button
        android:id="@+id/button_create"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/white"
        android:textColor="@color/black"
        android:text="Data Register User"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_margin="10dp"
        />

    <TextView
        android:id="@+id/namaLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nama"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="62dp" />

    <TextView
        android:id="@+id/nama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nama"
        tools:layout_editor_absoluteX="184dp"
        tools:layout_editor_absoluteY="89dp" />

    <TextView
        android:id="@+id/tanggalLahirLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tanggal Lahir"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="108dp" />

    <TextView
        android:id="@+id/tanggalLahir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tanggalLahir"
        tools:layout_editor_absoluteX="284dp"
        tools:layout_editor_absoluteY="143dp" />

    <TextView
        android:id="@+id/NomorTeleponLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nomor Telepon"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="143dp" />

    <TextView
        android:id="@+id/nomorTelepon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nomorTelepon"
        tools:layout_editor_absoluteX="305dp"
        tools:layout_editor_absoluteY="228dp" />

    <TextView
        android:id="@+id/emailLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="179dp" />

    <TextView
        android:id="@+id/email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="email"
        tools:layout_editor_absoluteX="304dp"
        tools:layout_editor_absoluteY="273dp" />
</androidx.constraintlayout.widget.ConstraintLayout>